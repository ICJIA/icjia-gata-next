    "markdown-it-named-headers": "0.0.4",
    "nuxt": "^2.8.1",
    "sitemap-generator": "^8.4.0",
    "slug": "^1.1.0",
    "vue-scrollto": "^2.15.0",
    "vuetify": "^1.5.14"
  },
  "devDependencies": {
    "nodemon": "^1.11.0",
    "babel-eslint": "^8.2.1",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
