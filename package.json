{
  "name": "eslint-nuxt",
  "version": "1.0.0",
  "description": "My beautiful Nuxt.js project",
  "author": "cschweda",
  "private": true,
  "scripts": {
    "dev": "kill-port 3000 && node generateManifest.js && nuxt",
